                        <Insets left="20.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </AnchorPane>
         </top>
         <bottom>
            <Separator opacity="0.0" orientation="VERTICAL" prefHeight="50.0" BorderPane.alignment="CENTER" />
         </bottom>
         <right>
            <Separator opacity="0.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <Separator opacity="0.0" prefWidth="50.0" BorderPane.alignment="CENTER" />
         </left>
      </BorderPane>
   </content>
</ScrollPane>
